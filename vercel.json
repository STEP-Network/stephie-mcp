{
	"buildCommand": "pnpm build",
	"outputDirectory": ".",
	"functions": {
		"api/server.ts": {
			"maxDuration": 300,
			"memory": 1024
		},
		"api/chatgpt.ts": {
			"maxDuration": 300,
			"memory": 1024
		},
		"api/mcp-chatgpt.ts": {
			"maxDuration": 300,
			"memory": 1024
		},
		"api/health.ts": {
			"maxDuration": 30
		}
	},
	"rewrites": [
		{
			"source": "/sse",
			"destination": "/api/server"
		},
		{
			"source": "/mcp",
			"destination": "/api/server"
		},
		{
			"source": "/message",
			"destination": "/api/server"
		},
		{
			"source": "/chatgpt",
			"destination": "/api/chatgpt"
		},
		{
			"source": "/chatgpt/sse",
			"destination": "/api/chatgpt"
		},
		{
			"source": "/chatgpt/message",
			"destination": "/api/chatgpt"
		},
		{
			"source": "/mcp-chatgpt",
			"destination": "/api/mcp-chatgpt"
		},
		{
			"source": "/mcp-chatgpt/sse",
			"destination": "/api/mcp-chatgpt"
		}
	],
	"headers": [
		{
			"source": "/api/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Credentials",
					"value": "true"
				},
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET,OPTIONS,POST,DELETE"
				},
				{
					"key": "Access-Control-Allow-Headers",
					"value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version, Authorization"
				}
			]
		}
	],
	"crons": [
		{
			"path": "/api/cron/sync-metadata",
			"schedule": "*/30 * * * *"
		}
	]
}
